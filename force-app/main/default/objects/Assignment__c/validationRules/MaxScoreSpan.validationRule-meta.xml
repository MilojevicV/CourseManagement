<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>MaxScoreSpan</fullName>
    <active>true</active>
    <errorConditionFormula>Max_Score__c &lt;= 0 || Max_Score__c &gt; 10</errorConditionFormula>
    <errorMessage>Max score should be between 1 and 10.</errorMessage>
</ValidationRule>
